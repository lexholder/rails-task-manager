<h1>Add a new task busy bee</h1>
<div id="back">
  <br><br><br>
<form action="/tasks" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="text" name="task[title]" placeholder="task">
  <br>
  <input type="text" name="task[details]" placeholder="details">
  <br>
  <input type="submit" class="card">
</form>
</div>
